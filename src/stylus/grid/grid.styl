/*
*	Grid System
*	Author - inArt
*	Site - http://inart.co.il
*/

*, *:before, *:after {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

[class*='col-'] {
	min-height: 1px;
	float: left;
}

.container {
	width: 1140px;
	margin: 0 auto;
	max-width:100%;
}

.container.main-menu-container {
	width: 1490px;
}

.container.head-container {
	width: 1220px;
}

.container.connect-container {
	width: 1255px;
}

.container.wide {
	width: 1400px;
}

.container.narrow {
	width: 1040px;
}

.grid {
	margin: 0;
}

.grid:before,
.grid:after,
.clear:before,
.clear:after {
	content: "";
	display: table;
	clear: both;
}

